import java.util.Scanner;

public class Exerc005 {
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        double notaA = 0;
        double pesoA = 3.5;
        double notaB = 0;
        double pesoB = 7.5;
        double media = 0;
        
        notaA = in.nextDouble();
        in.nextLine();
        notaB = in.nextDouble();
        in.nextLine();
        
        media = ((notaA * pesoA) + (notaB * pesoB)) / (pesoA + pesoB);
        
        System.out.printf("MEDIA = %.5f\n", media);
    }
}
