import java.util.Scanner;

public class Uri1006 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int pesoA = 2;
		int pesoB = 3;
		int pesoC = 5;
		double a, b, c, media;
		a = b = c = media = 0;
		
		a = in.nextDouble();
		in.nextLine();
		
		b = in.nextDouble();
		in.nextLine();
		
		c = in.nextDouble();
		in.nextLine();
		
		media = ((a * pesoA) + (b * pesoB) + (c * pesoC)) / (pesoA + pesoB + pesoC);
		System.out.printf("MEDIA = %.1f\n", media);
	}
}
