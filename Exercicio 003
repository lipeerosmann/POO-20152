import java.util.Scanner;

public class Exerc003 {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int a = 0
		int b = 0;
		int soma;
		a = in.nextInt();
		in.nextLine();
		
		b = in.nextInt();
		in.nextLine();
		
		 soma = a + b;
				
		System.out.println("SOMA = " + soma);
		
		
	}
}
